# Database Connection
DATABASE_URL=postgresql://user:password@host:port/database?sslmode=require

# Encryption Key (generate with: openssl rand -base64 32)
# Must be at least 32 characters long
ENCRYPTION_KEY=your-32-character-or-longer-encryption-key-here

# Default User Credentials (128 characters each)
# These will be used to create the single user account on first initialization
DEFAULT_USERNAME=your-128-character-username-here
DEFAULT_PASSWORD=your-128-character-password-here

# Node Environment
NODE_ENV=development
